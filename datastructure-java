package javadatastructure;


	class node
	{
	int data;
	node next;
	node(int a)
	{
	data=a;
	next=null;
	}
	}
	public class javadata
	{
	static node head,first,temp,second,third;
	public static void main(String a[])
	{
	javadata j=new javadata();
	head=new node(10);
	first=new node(20);
	second=new node(30);
	third=new node(40);
	head.next=first;
	first.next=second;
	second.next=third;
	third.next=null;
		j.display(head);
	}
	
	void display(node temp)
	{ 
	while(temp!=null)
	{
	System.out.println(temp.data);
	temp=temp.next;
	}
	}
	}
	
